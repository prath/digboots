            // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
            // [ COLLAPSIBLE SEARCH BOX ]
            // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
            .search.box
                form(action='', method='', role='form')
                    label.sr-only(for='#searchinput') Email address
                    input#searchinput.searchinput(type='search', placeholder='Type Query Here...')
                    button(type='submit').searchsubmit
                        i.fa.fa-search

            //- // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
            //- // [ COLLAPSIBLE CART ]
            //- // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
            //- aside.right.positioned.cartbar
                .cart.content 
                
                    // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
                    // [ WIDGET SHOPPING CART ]
                    // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
                    include ../widgets/widget-woo-cart

        // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
        // [ LEAD AND LOGO ]
        // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
        //- include ../inc/masthead
        header#left.left
            .masthead
                .content
                    h1.sr-only Digboots
                    .logo
                        a(href="")
                            img(src="dummy-images/logo.png", alt="")
                    div.collapse-header
                        a(href="")
                            i.fa.fa-angle-double-down
                    p.lead Clarity, Simplicity and Curiousity.
                    p 
                        | We define every job as <strong>clarity</strong>, <strong>simplicity</strong>&nbsp;and <strong>curiousity</strong>.
                    section.panel.panel-default.widget.widget_social
                        header.panel-heading
                            h3.widget.heading Connect With Us
                        .panel-body
                            .social.icons
                                a.social.icon(href='')
                                    .diamond.icon
                                        i.fa.fa-facebook
                                a.social.icon(href='')
                                    .diamond.icon    
                                        i.fa.fa-twitter
                                a.social.icon(href='')
                                    .diamond.icon
                                        i.fa.fa-google-plus
                                a.social.icon(href='')
                                    .diamond.icon
                                        i.fa.fa-linkedin
                                a.social.icon(href='')
                                    .diamond.icon
                                        i.fa.fa-github
        

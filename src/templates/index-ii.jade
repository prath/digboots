    // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    section.content.shrunk-all#contact
        .narrow
            h2.section.heading
                | Want to work with us?
            p.centered
                | Are you working on something great? We'd love to help make it happen.
            form(action='', role='form')
                .form-group
                    label.sr-only(for='contactname')
                    input.form-control(type='text', id='contactname', placeholder='Name', tabindex='1')
                .form-group
                    label.sr-only(for='contactemail')
                    input.form-control(type='text', id='contactemail', placeholder='Email', tabindex='2')
                .form-group
                    label.sr-only(for='contactweb')
                    input.form-control(type='text', id='contactweb', placeholder='Website', tabindex='3')
                .form-group
                    label.sr-only(for='contactmsg')
                    textarea.form-control(id='contactmsg', cols="50", rows="10", placeholder='Message', tabindex='4')
                button.btn.btn-primary.pull-right
                    | Send Message 